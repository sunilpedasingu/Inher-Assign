
public class ExampleInher {

	public static void main(String[] args) {
		for(int n=1;n<=5;n++)
		{
			for(int m=0;m<=n;m++)
			{
				System.out.print(n);
			}System.out.println(n);
		}
		
		
		for(int i=5;i>=0;i--)
		{
			for(int j=1;j<=i;j++)
			{
			System.out.print(i);
			}
		}System.out.println();
	}

}
